<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="GsCg.petfinder.ui.feed.FeedFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_feed" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="GsCg.petfinder.ui.lostpet.LostFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_lost" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="GsCg.petfinder.ui.findpet.FindFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_find" />

